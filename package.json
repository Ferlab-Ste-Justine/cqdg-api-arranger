{
  "name": "cqdg-api-arranger",
  "version": "1.1.0",
  "description": ":factory: a data-model aware GraphQL API that sits above an Elasticsearch cluster",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./dist/src/index.js",
    "dev": "ts-node-dev ./src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf ./dist/* ",
    "build": "npm run clean; tsc && cp -r admin dist/admin",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "buildAndRun": "npm run build; npm start",
    "admin-project": "node --experimental-modules=node --es-module-specifier-resolution=node admin/run.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ferlab-Ste-Justine/cqdg-api-arranger.git"
  },
  "keywords": [
    "arranger"
  ],
  "author": "",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/Ferlab-Ste-Justine/cqdg-api-arranger/issues"
  },
  "homepage": "https://github.com/Ferlab-Ste-Justine/cqdg-api-arranger#readme",
  "dependencies": {
    "@arranger/admin": "^2.17.0",
    "@arranger/middleware": "^2.16.0",
    "@arranger/server": "^2.16.1",
    "@awaitjs/express": "^0.8.0",
    "@elastic/elasticsearch": "^7.9.1",
    "aws-sdk": "^2.1035.0",
    "cors": "2.8.5",
    "dateformat": "^5.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "filesize": "^3.6.1",
    "graphql-parse-resolve-info": "4.11.0",
    "http-status-codes": "^2.1.4",
    "keycloak-connect": "^14.0.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.5",
    "python-shell": "^3.0.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.3.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.0.6",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.19",
    "prettier": "^1.19.1",
    "supertest": "^6.1.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
